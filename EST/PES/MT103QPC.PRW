User Function MT103QPC()

Local aArea  := GetArea()
Local zQuery := PARAMIXB[1]

If !u__UsrMpOk()
	zPosOrd := AT("ORDER",zQuery)
	zQryRet := Substr(zQuery,1,zPosOrd-1)
	zQryRet += " AND C7_ZZMP <> 'X' "
	zQryRet += Substr(zQuery,zPosOrd)
	zQuery  := zQryRet
Endif

RestArea(aArea)

Return(zQuery)
