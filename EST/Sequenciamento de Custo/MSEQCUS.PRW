User Function MSEQCUS()

Private cCadastro := "Sequenciamento do Custo Médio"

Private aRotina   := {{"Pesquisar"  , "AxPesqui"    , 0 , 1 },;
            	      {"Visualizar" , "AxVisual"    , 0 , 2 },;
            	      {"Incluir"    , "U_MSEQ10I()" , 0 , 3 },;
            	      {"Alterar"    , "U_MSEQ10A()" , 0 , 4 },;
            	      {"Excluir"    , "U_MSEQ10E()" , 0 , 5 }}

If !(__cUserId $ ( AllTrim(GetMv("MV_XGEREST")) )) // Administrador, Jefferson Costa
	MsgAlert("Usuário não autorizado. Contate o Administrador.")
	Return
Endif

DbSelectArea("SZP")
DbSetOrder(1)
MBrowse(6,1,22,75,"SZP",,,)

Return

