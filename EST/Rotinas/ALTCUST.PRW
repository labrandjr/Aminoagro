User Function ALTCUST()

Private cCadastro := "Alteração do Custo Total"

Private aRotina   := {{ "Pesquisar"  , "AxPesqui" , 0, 1},;
   					  { "Visualizar" , "AxVisual" , 0, 2},;
   					  { "Incluir"    , "U_ACST01I", 0, 3},;
   					  { "Alterar"    , "U_ACST01A", 0, 4},;
   					  { "Excluir"    , "U_ACST01E", 0, 5}}

Private _mvUserEst := AllTrim(GetMv("ZZ_USEREST"))

// Verifica se o usuário é o Administrador do sistema ou usuários autorizados.
If !Alltrim(__cUserID) $(_mvUserEst)
	Alert("Somente o Administrador ou usuários autorizados podem executar esta rotina.")
	Return
EndIf   

oMBrowse := FWMBrowse():New()
oMBrowse:SetAlias( "SD1" )			
oMBrowse:SetDescription( cCadastro )
oMBrowse:Activate()

DbSelectArea("SD1")
DbSetOrder(1)
DbClearFilter()

Return

