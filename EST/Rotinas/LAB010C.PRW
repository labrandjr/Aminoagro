/*/

Static Function VerLctoOk()

Local aArea  := GetArea()
Local lRetOk := .T.

cQuery := " SELECT COUNT(*) AS SOMA "
cQuery += " FROM "+RetSqlName("CT2")
cQuery += " WHERE CT2_FILIAL = '"+xFilial("CT2")+"' "
cQuery += " AND CT2_LOTE = '008840' "
cQuery += " AND SUBSTR(CT2_DATA,1,6) = '"+Substr(DtoS(SZ7->Z7_EMISSAO),1,6)+"' "
cQuery += " AND D_E_L_E_T_ <> '*' "
DbUseArea( .T., "TOPCONN", TCGENQRY(,,cQuery),"SQL", .F., .T.)
lRetOk := (SQL->SOMA == 0)
SQL->(DbCloseArea())

If !lRetOk
	MsgAlert("Atenção, existem lançamentos contábeis no período."+CHR(13)+CHR(10)+"Operação não permitida.")
Endif

RestArea(aArea)

Return(lRetOk)
