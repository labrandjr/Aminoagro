User Function USRACMP()

Private cCadastro := "Usuários x MP"

Private aRotina   := {{"Pesquisar"  , "AxPesqui"    , 0 , 1 },;
            	      {"Visualizar" , "AxVisual"    , 0 , 2 },;
            	      {"Incluir"    , "U_USRAMPI()" , 0 , 3 },;
            	      {"Alterar"    , "U_USRAMPA()" , 0 , 4 },;
            	      {"Excluir"    , "U_USRAMPE()" , 0 , 5 }}

If !(__cUserId $ AllTrim(GetMv("MV_USMPMAS"))) // Administrador, Jefferson Costa, Paul Bergman
	MsgAlert("Usuário não autorizado. Contate o Administrador.")
	Return
Endif

DbSelectArea("SZH")
DbSetOrder(1)
MBrowse(6,1,22,75,"SZH",,,)

Return

