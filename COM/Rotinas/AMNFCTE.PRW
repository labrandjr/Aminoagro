User Function AMNFCTE()

Local zCondicao := " Substr(F1_ESPECIE,1,3) == 'CTE' "

Private cCadastro := "Manutenção de Documento de Entrada CTE"

Private aRotina   := {{ "Pesquisar"  , "AxPesqui" , 0, 1},;
   					  { "Visualizar" , "AxVisual" , 0, 2},;
   					  { "Incluir"    , "U_AMNCTEI", 0, 3},;
   					  { "Alterar"    , "U_AMNCTEA", 0, 4},;
   					  { "Excluir"    , "U_AMNCTEE", 0, 5}}

DbSelectArea("SF1")
DbSetOrder(1)
Set Filter to &zCondicao
SF1->(DbGotop())
MBrowse(6,1,22,75,"SF1",,"F1_UFORITR",20)

DbSelectArea("SF1")
DbSetOrder(1)
Set Filter to

Return

