/*/

User Function LogPremi(a3Cod, a3Nom, a3Pdf, a3Eml, a3Obs, a3Ori, a3Tip)

DbSelectArea("LOG")
DbSetOrder(1)
If !DbSeek( a3Cod + a3Ori + a3Tip )
	RecLock("LOG",.T.)
	LOG->LOG_COD := a3Cod
	LOG->LOG_ORI := a3Ori
	LOG->LOG_TIP := a3Tip
	LOG->LOG_NOM := a3Nom
	LOG->LOG_PDF := a3Pdf
	LOG->LOG_EML := a3Eml
	LOG->LOG_OBS := "E-mail não enviado."
	MsUnLock()
Else
	If !Empty(a3Obs)
		RecLock("LOG",.F.)
		LOG->LOG_EML := a3Eml
		LOG->LOG_OBS := a3Obs
		MsUnLock()
	Endif
Endif

Return
