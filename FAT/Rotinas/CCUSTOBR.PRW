User Function CCUSTOBR()

Local aArea    := GetArea()
Local cRetorno := ""

If AllTrim(M->C6_PRODUTO) == "SERV0001"
	cRetorno := "106040801001"
ElseIf !Empty(M->C5_VEND1)
	DbSelectArea("SA3")
	DbSetOrder(1)
	If DbSeek( xFilial("SA3") + M->C5_VEND1 )
		cRetorno := SA3->A3_ZZCC
	Endif
Endif

RestArea(aArea)

Return(cRetorno)
