User Function MT450MAN()

Local aArea   := GetArea()
Local zUsCrMn := AllTrim(GetMv("MV_XUSCRMN"))
Local lRetOk  := .T.

If __cUserId != "000000"
	If !Empty(zUsCrMn)
		If !(__cUserId $ zUsCrMn)
			MsgInfo("Atenção, liberação manual de crédito não autorizada para este usuário. Contate o Administrador.")
			lRetOk := .F.		
			Return
		Endif
	Endif	
Endif

RestArea(aArea)

Return(lRetOk)
