User Function FA080LIB()

Local aArea  := GetArea()
Local lGrava := .T.

If Alltrim(CMOTBX) = "NORMAL"

	If !MsgYesNo('Esse motivo de baixa não gera contabilização. Continua?','Atenção!')
		lGrava := .F.
	Endif

Endif

RestArea(aArea)

Return(lGrava)
